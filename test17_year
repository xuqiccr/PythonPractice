#coding = utf-8
def run(years):
    if years % 100 == 0:
        if years % 400 != 0:
            print '%d为平年' % (years)
        else:
            print '%d为闰年' % (years)
        
    else:
        if years % 4 == 0:
            print '%d为闰年' % (years)
        else:
            print '%d为平年' % (years)
    
year = int(raw_input('请输入年份：'))
run(year)
